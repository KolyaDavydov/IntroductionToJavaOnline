package algorithmization_2.arraysOfArrays;

public class Task8 {

	public static void main(String[] args) {
		int n = 8;
		int[][] matrix = new int[n][n];
		for(int i = 0; i < n; i++) {
			for(int j = 0; j < n; j++) {
				matrix[i][j] = j + 1;
				
			}
		}
		
		System.out.println("Имеется исходный массив:");
		for(int i = 0; i < n; i++) {
			for(int j = 0; j < n; j++) {
				System.out.print(" " + matrix[i][j] + " ");
			}
			System.out.println();
		}

	}

}

