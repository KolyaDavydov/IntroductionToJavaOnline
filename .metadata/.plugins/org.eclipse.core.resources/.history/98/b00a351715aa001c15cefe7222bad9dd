package algorithmization_2.arraysOfArrays;

import java.util.Arrays;

public class Task8 {

	public static void main(String[] args) {
		int n = 8;
		int[][] matrix = new int[n][n];
		for(int i = 0; i < n; i++) {
			for(int j = 0; j < n; j++) {
				matrix[i][j] = (int) (Math.random() * 10);
			}
		}
		for(int i = 0; i < n; i++) {
			System.out.println(Arrays.toString(matrix[i]));
		}
		

	}

}

